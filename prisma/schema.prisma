generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @db.VarChar(100)
  name  String? @db.VarChar(100)
}

model Event {
  id Int @id @default(autoincrement())
}

model asdf {
  id Int @id @default(autoincrement())
}
